    // Hide hotspot logos, this doesnt fix the top margins,
    // its impossible to pick .center for these without picking all unrelated .centers
    if (!(hide_hsl)) {
        /img {
            &[src*='sputnik'],
            &[src*='hotspotsystem'],
            &[src*='wifidog'] {
                display: none
            }
        }
    }

    // Paypal logo inversion
    [type='image' i][src*='paypal'] {
        background: t
        border-color: t
        filter: invert(.9) hue-rotate(180deg)
        margin-bottom: -.5rem
        mix-blend-mode: exclusion

        &:hover {
            background: fg1
            border-color: t
        }
    }

    // Wi-viz survey
    .logo[src*='wiviz'] {
        filter: invert(.95) hue-rotate(180deg) contrast(1.6)
        mix-blend-mode: lighten
    }

    .controls {
        /img {
            /[src*='wiviz'] {
                background-color: t
                filter: invert(.95) hue-rotate(180deg) contrast(1.6) opacity(.5)
                mix-blend-mode: lighten
            }
            /[src*='up'], /[src*='down'], /[src*='zoom'], /[src*='reset'] {
                &:hover, &:focus-within {
                    background-color: t
                    filter: invert(.95) hue-rotate(180deg) contrast(1.6) opacity(1)
                    mix-blend-mode: lighten
                }
            }
        }
    }

    svg { shape-rendering: geometricPrecision }

    // Bandwidth graph
    /#graph {
        width: clamp(200px, 50% + 20px, 800px)
        text {
            /[fill='green'] { g: v44 }
            /[fill='red'] { g: v16 }
            /[fill='#435370'] { g: v68 0 }
            /[fill='blue'] { g: v65 }
            /[fill='gray'] { g: v96 }
        }
        path {
            /[stroke='red'] { g: 0 v16 }
            /[stroke='green'] { g: 0 v44}
            /[stroke='gray'] { g: 0 bg5; stroke-opacity: .5 }
        }
        rect {
            /#bg[fill='white'] { g: bg2; fill-opacity: 1 }
        }
        line[stroke="black"] { g: 0 bg5 }
    }

    // Traffic graph
    #t-graph {
        background: bg0
        border: 1px solid bg5
        color: fg3

        li {
            &.day {
                border-right: 1px dotted bg5; color: fg3

                &_sun { border-right: 1px dotted bg5; color: v16 }
            }

            .rcvd { background: v43; outline: 1px ridge v44; outline-offset: -2px }
            .sent { background: v15; outline: 1px ridge v17; outline-offset: -2px }
            .bar { color: rgba(BG, .5); outline: 0 }
        }
        #ticks .tick { border-bottom: 1px solid bg5 }
    }

    .overlay {
        &_dialog { c: 0 0 bg4 }

        &___invisible__ { background-color: bg4 i }
    }

    .dialog {
        &_ { filter: invert(1) }

        &_content {
            color: fg1
            background-color: bg0
        }
    }

    // Progress dialog popup when applying settings
    .ddwrt {
        &_content {
            background: bg1
            color: fg1
        }

        &_progress {
            filter: invert(1) grayscale(1)
            mix-blend-mode: exclusion
        }

        &_wired_frame { background: bg4 }

        &_window {
            background: bg0
            border: 1px solid v16
        }

        &_title { color: v65 }

        &_nw, &_n, &_e, &_sw, &_ne,
        &_w, &_s, &_se, &_sizer { background: bg1 }
    }

    // Progress bar when e.g. reboot
    .progressbarblock {
        background-color: v09
    }

    .overlay_ddwrt {
        background-color: bg3
        opacity: .5
    }

    // Better checkbox/radio inputs.
    /input {
        border: 1px solid bg4
        b: 0 1px 1px rgba(BG, .075)

        transition: transition
        padding: 7px
        line-height: 1.2em
        &:hover, &:focus-within  {
            border-color: v08
            background-color: bg1
        }

        appearance: none
        -moz-appearance: none
        -webkit-appearance: none

        prefix  = 'data:image/svg+xml;utf8,'
        prefix += '<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">'

        &[type='radio' i],
        &[type='checkbox' i] {
            &, &:focus, &:hover:active {
                c: v95 bg5 bg1

                bottom: .15rem
                background-size: cover
                cursor: pointer
                position: relative
                height: b_hgwd
                width: b_hgwd

                vertical-align: middle
            }
            &:disabled, &:hover:active:disabled, &:disabled:hover {
                c: v95 bg5 bg4

                opacity: .7
                cursor: default
            }
        }

        &[type='radio' i] {
            // fix border radius at https://github.com/settings/profile Emoji color
            border-radius: 50%
            &:checked {
                path = '<circle cx="256" cy="256" r="128" fill="' + to_svg($v99) + '"/>'

                background-image: s("url('%s%s</svg>') %s", rq(prefix), rq(path), i)
                c: 0 v05 v09

                padding: 7px
            }
            &:focus { border-radius: 50% }
            &:hover { c: 0 v08 bg2 }
        }

        &[type='checkbox' i] {
            border-radius: 2px
            &:checked {
                path ='<path fill="' + to_svg($v99) + '" '
                path += 'd="M99 213l-1 89 104 102 205-201v-91L203 315 99 213z"/>'

                background-image: s("url('%s%s</svg>') %s", rq(prefix), rq(path), i)
                c: 0 v05 v09
                border-radius: 3px
            }
            &:hover { c: 0 v08 bg2 }
            &:indeterminate {
                path = '<rect x="128" y="128" width="256" height="256" fill="' + to_svg($v09) + '"/>'

                background-image: s("url('%s%s</svg>') %s", rq(prefix), rq(path), i)
                c: 0 v16 v14
            }
        }
        &[type='number' i] { } // TODO: style spinbox
        // Style file browse button
        &[type='file' i] {
            &::file-selector-button {
                background-color: bg5
                border: 2px solid bg5
                color: v98

                border-radius: .32em
                padding: .2rem .4rem
                transition: transition
                cursor: pointer
                &:hover {
                    background-color: v06
                    border: 2px solid v06
                    box-shadow: 0 0 10px 0 v02
                    color: v98
                }
            }
            &::-webkit-file-upload-button {
                background-color: bg5
                border: 2px solid bg5
                color: v98

                border-radius: .32em
                padding: .2rem .4rem
                transition: transition
                cursor: pointer
                &:hover {
                    background-color: v06
                    border: 2px solid v06
                    box-shadow: 0 0 10px 0 v02
                    color: 98
                }
            }
        }
        &[disabled] {
            opacity: .7
        }
        /.off {
            background: bg2
            opacity: .99
            input {
                color: v94 i
                border-color: bg4
                background-color: bg0
                opacity: .5
                /.num.off { opacity: .5 }
            }
        }
    }

    .row[role="controls"] { border-color: bg4 }
    .progress {
        background-color: bg4
        box-shadow: inset 0 1px 2px rgba(BG, .1)
        -webkit-box-shadow: inset 0 1px 2px rgba(BG, .1)

        r: 4px
        &-bar.odd {
            background-color: v08
        }
    }

    // Better select element styles.
    /select {
        // Custom select dropdown icon.
        prefix  = 'data:image/svg+xml;utf8,'
        prefix += '<svg width="14" height="14" viewBox="0 0 14 12" fill="' + to_svg($v97) + '" '
        prefix += 'xmlns="http://www.w3.org/2000/svg">'
        path    = '<path d="M6 9a1 1 0 0 1-.707-.293l-3-3a1 1 0 0 1 1.414-1.414L6'
        path   += ' 6.586l2.293-2.293a1 1 0 0 1 1.414 1.414l-3 3A1 1 0 0 1 6 9z"/>'
        xy      = 'no-repeat right 3px center'

        background: s("url('%s%s</svg>') %s", rq(prefix), rq(path), rq(xy) i)
        background-color: bg0 i
        border: 1px solid bg4
        b: 0px 1px 1px rgba(BG, .075)
        color: v98

        appearance: none
        -moz-appearance: none
        -webkit-appearance: none
        height: 1.7rem
        max-width: 100%
        outline: 0
        padding-right: 1.5rem
        > option {
            background-color: bg0 i
            &:hover {
                background: v04 i
                box-shadow: 0 0 10px 100px v06 inset i
            }
            /[selected] {
                background-color: v06 i
                box-shadow: 0 0 10px 100px v06 inset i
            }
        }
        &:focus > option:hover
        &:focus-within  { c: 0 v08 bg1 }
        &:hover {
            prefix  = 'data:image/svg+xml;utf8,'
            prefix += '<svg width="14" height="14" viewBox="0 0 14 12" fill="' + to_svg($v99) + '" '
            prefix += 'xmlns="http://www.w3.org/2000/svg">'
            path    = '<path d="M6 9a1 1 0 0 1-.707-.293l-3-3a1 1 0 0 1 1.414-1.414L6'
            path   += ' 6.586l2.293-2.293a1 1 0 0 1 1.414 1.414l-3 3A1 1 0 0 1 6 9z"/>'
            xy      = 'no-repeat right 3px center'

            background: s("url('%s%s</svg>') %s", rq(prefix), rq(path), rq(xy) i)
            c: 0 v08 bg1

            transition: transition
        }

        &[disabled] {
            prefix  = 'data:image/svg+xml;utf8,'
            prefix += '<svg width="14" height="14" viewBox="0 0 14 12" fill="' + to_svg($v93) + '" '
            prefix += 'xmlns="http://www.w3.org/2000/svg">'
            path    = '<path d="M6 9a1 1 0 0 1-.707-.293l-3-3a1 1 0 0 1 1.414-1.414L6'
            path   += ' 6.586l2.293-2.293a1 1 0 0 1 1.414 1.414l-3 3A1 1 0 0 1 6 9z"/>'
            xy      = 'no-repeat right 3px center'

            background: s("url('%s%s</svg>') %s", rq(prefix), rq(path), rq(xy) i)
            c: v94 bg4 bg0

            opacity: .5
        }

        // QoS fix service name table width due to custom select menus.
        /[name='ServiceTable'] {
            // dont show arrow select arrow in this dialog
            background: none i
            background-color: bg0 i
            border: 1px solid bg4

            width: inherit
            height: @width
            option {
                width: 200%
                &:hover { background-color: v02 i }
                &:checked {
                    background-color: v05 i
                    box-shadow: 0 0 10px 100px v06 inset
                }
            }
            &:hover {
              // dont show arrow select arrow in this dialog
              background: none i
              background-color: bg0 i

            }
        }
    }

    // improve trash bin
    .bin {
        prefix  = 'data:image/svg+xml;utf8,'
        prefix += '<svg viewBox="0 0 16 16" width="16" height="16" fill="' + to_svg($v97) + '" '
        prefix += 'xmlns="http://www.w3.org/2000/svg">'
        path    = '<path fill-rule="evenodd" d="M6.5 1.75a.25.25 0 01.25-.25h2.5a.25.25 0 01.25'
