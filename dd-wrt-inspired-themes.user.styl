                &:hover { background-color: bg3 }
            }
            /[bgcolor='#FFFFFF'] { background-color: bg0 }
            /[bordercolor='#000000'] { border-color: bg4 }
        }
        tr[bgcolor='#CCCCCC'] {
            background-color: bg2
            &:hover, &:focus-within { background-color: bg3 }
        }
        // syslog color
        tr[bgcolor='#7CFC00'] {
            background-color: v42
            color: v99
        }
        // Nas - add margin before submitFooter
        /#drives { margin-bottom: 6px }
        /[summary='packet-type priority table'] {
            td[colspan='5'] { padding-bottom: 6px }
        }
        /#conntrack_table {
            a:link {
                color: v97
                &:hover { color: v08 }
            }
        }

        .bulle { border-color: bg5 }
    }

    .odd { background-color: bg2 }

    .status {
        &_red {
            background: v16
            color: v99

            // padding spreads to all below
            padding 2px
            r: 2px
        }
        &_green {
            background: v42
            color: v99
            r: 2px
        }
        &_yellow {
            background: v34
            color: v99
            r: 2px
        }
        &_orange {
            background: v26
            color: v99
            r: 2px
        }
    }

    .warning {
        background: v11
        border: 2px dashed v15
        text-shadow: 0 1px 0 v10
        #warning_text, p b {
            font-size: 1rem
            letter-spacing: .15rem
            margin-top: -.6rem
            margin-bottom: -.8rem
        }
    }

    // Help / Wi-viz / bulle aka tooltip
    div {
        // HELP
        &.also {
            background: bg2
            border-color: bg4
        }
        .bulle {
            background-color: bg5
            color: v99
        }
        // Purple note
        .note {
            background: v71
            border-color: v75

            margin-top: .5rem
        }
        // Wi-vis
        &.gridpoint { background-color: fg1 }

        &.slidingheader {
            background-color: bg2
            background-image: none
            border: 1px solid bg4
        }
        &.slidingbodyshow {
            background-color: bg2

            center { border-color: bg4 }
        }
        &.slidingbody { background-color: bg5 }
    }

    // Wi-viz client MAC bg
    span.title3d {
        background-color: bg0
    }

    // Setting labels
    .setting {
        // inline style
        span {
            &[style*='color:#999999'] { c: v96 0 0 }
        }

        .default { color: v96 0 0 }
    }

    a {
        color: v08
        &:hover, &:focus-within { color: v09 }
    }

    h2, h3 {
        background: bg5
        border-color: bg5
        color: fg1
        letter-spacing: .95px
    }

    // Submit options button footer
    .submitFooter {
        // BG color was decided to increase contrast in NAS tab placement of submitFooter bg over bg
        background-color: rgba(BG, .5)
        border: 1px solid bg4
        color: fg1
    }

    // Meter
    .meter {
        background-color: bg4
        border-color: bg5

        .text {
            if (light(AC)) {
                color: v98
                text-shadow: 0 1px 0 v90
            }
            else {
                color: v93
                text-shadow: 0 1px 0 v90
            }
        }

        .bar { background-color: v05 }
    }

    // DD_WRT bottom link spacing
    .center a[href='http://www.dd-wrt.com/'] {
        line-height: 2.2rem
    }

    // Images + nav
    #header {
        // Help header
        background: bg
        // DDWRT logo
        #logo {
            filter: contrast(1.2)
            mix-blend-mode: lighten
        }
        .logo {
            filter: invert(.95) hue-rotate(180deg)
            mix-blend-mode: lighten
        }
        .navig {
            a {
                color: fg3
                &:hover, &:focus-within { color: fg1  }
            }
        }
    }

    // DD-WRT logo in about dialog
    [src*='dd-logo.png'] {
        filter: invert(.95) hue-rotate(180deg)
        mix-blend-mode: lighten
    }

