          background-color: bg5 i
        }
        .t-border {
          border-color: bg4 i
        }
      }
      a {
        color: v09 i
        &:hover {
          color: v08 i
        }
      }
    }
    /.help-bg {
      background: bg2 i
      #content {
        background: t
        border-color: t
        color: v98
        h2 {
          background: bg5 i
          color: fg1 i
        }
      }
      a {
        color: v98
        &:hover {
          color: v09
        }
      }
      dd { border-bottom: .0648rem dotted fg5 }
    }
    // Reboot message box when classic dark styles enabled
    .message {
      background-color: bg2
      border-color: bg3
      rad: 4px
    }
  }

