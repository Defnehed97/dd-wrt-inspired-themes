    div.center {
        input.button {
            &[name='add_button']:not([onclick*='_add']) {
                margin: 0 0 .8em 0
            }
        }
    }
    input {
        &.button {
            &[onclick*='copy'],
            &[onclick*='past'] {
                margin: 0 0 .8em 0
            }
            &[onclick*='vifs_add'] {
                margin: .4em 0 0 0
            }
            &[onclick*='delete'] {
                margin: 0 .4em 0 .4em
            }

            // Tunnels - generate key button position
            &[onclick*='gen_wg_key'] {
                margin: 0 0 6px -32%
            }
            &[onclick*='add_peer'] {
                margin: 0 0 -6px -35%
            }
            // better padding for preview style button
            &[onclick*="previewStyle"] {
                padding: .362em 1.087em
            }
            //vlan add button tweaks
            &[onclick*="vlan_add"],
            &[onclick*="bridge_add"],
            &[onclick*="bridgeif_add"],
            &[onclick*="ipvs_add"],
            &[onclick*="bond_add"],
            &[onclick*="mdhcp_add"] {
                padding: .2em .95em
            }
        }

        // Fix specific checkbox margin wifi security unmask checkbox
        &[type='checkbox' i] {
            &[onclick*='setElementMask'] {
