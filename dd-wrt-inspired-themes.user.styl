@-moz-document regexp('https?://iplookup.flagfox?.net(/.*)?') {
// iplookup.flagfox.net
  body {
    background: bg2
    overflow-x: hidden
    input {
      rad: 4px
      &[type='submit'] {
        background-color: v04
        border-color: v06
        color: v99
        padding: 0 1rem
        height: 1.733rem
        cursor: pointer
        &:hover {
          background-color: v07
          border-color: v07
          color: v99
        }
      }
    }
  }

  table {
    tr:hover { background-color: t i }
    &.table-heading {
      td[align='center'] h1 a[href='/'] { color: v97 }
      &:not(:focus) { c: 0 0 bg2 }
      .form img[src] {
        vertical-align: middle
      }
    }
    &.table-geotool {
      width: 100%
      margin-left: 3em
      td[align='left'] {
        width: 360px
      }
      td[width='25%'] a b {
        max-width: 380px
        display: inline-block
        text-overflow: ellipsis
        overflow-x: hidden
        vertical-align: middle
      }
    }
  }

  a {
    &:link { color: v06 }
    &:hover, &:focus-within { color: v09 }
  }

  iframe[src*='/maps/embed'] {
    min-width: 800px i
    filter: invert(1) hue-rotate(180deg) contrast(.9)
    &:hover { filter: invert(.98) hue-rotate(180deg) contrast(.8) }
  }

  .google-map {
    min-width: 900px i
    margin: 0 auto
  }
  .adsbygoogle {
    display: none i
  }

  .hr2, hr {
    border-top: 1px solid bg5
    border-bottom: 0
    background-color: t i
  }

  .gm-inset {
    &-dark {
      background-color: bg2 i
      border-color: bg4 i
    }
    &-light {
      background-color: bg2 i
      border-color: bg4 i
    }
  }

  select {
    line-height: 1.7 i
    rad: 4px
    option { color: v99 }
  }
}
