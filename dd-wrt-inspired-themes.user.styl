@-moz-document regexp('https?://iplookup.flagfox?.net(/.*)?') {
    // iplookup.flagfox.net
    body {
        background: bg0
        input {
            r: 4px
            &[type='submit' i] {
                background-color: bg4
                border-color: bg4
                color: v99
                &:hover {
                    background-color: bg5
                    border-color: bg5
                    color: v99
                }
            }
        }
    }

    table {
        tr:hover { background-color: bg0 i }
        &[class="table-heading"] {
            td[align='center'] h1 a[href='/'] { color: v65 }
            &:not(:focus) { c: 0 0 bg0 }
        }
    }

    a {
        &:link, &:visited { color: v06 }
        &:hover, &:focus-within { color: v09 }
    }

    iframe[src*='https://www.google.com/maps/embed'] {
        filter: invert(1) hue-rotate(180deg) contrast(.9)
        &:hover { filter: invert(1) hue-rotate(180deg) contrast(.9) }
    }

    .hr2, hr {
        border-top: 1px solid bg4 //#ccc;
        border-bottom: 1px solid bg4 //#efefef;
    }

    .gm-inset-dark {
	    background-color: bg2
	    border-color: bg4
    }

    select {
        r: 4px
        option { color: v99 }
    }
}
